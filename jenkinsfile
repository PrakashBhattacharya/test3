pipeline{
    agent any
    tools{
        maven 'maven'
    }
    stages{
        stage('install'){
            steps{
            bat 'npm install'
            }
        }
        stages{
        stage('tests'){
            steps{
            bat 'npm test || echo "No tests defined"'
            }
        }
    stages{
        stage('built'){
            steps{
             bat 'npm run build'  
            }
        }
        /*stage('PORT'){
            steps{
                script{
                   echo "Server is running on port ${PORT}" 
                }
        }
    }*/
}
}
}
}